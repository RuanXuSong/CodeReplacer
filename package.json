{
  "name": "css-code-replacer",
  "displayName": "cssCodeReplacer",
  "description": "replace css styles as you like",
  "version": "0.1.0",
  "author": "rxs-michael",
  "publisher": "rxs-michael",
  "engines": {
    "vscode": "^1.32.0"
  },
  "icon": "logo.png",
  "keywords": [
    "css",
    "code",
    "replacer",
    "code replacer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RuanXuSong/CodeReplacer.git"
  },
  "homepage": "https://github.com/RuanXuSong/CodeReplacer/blob/master/README.md",
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.codeReplacer.cssToCamel",
    "onCommand:extension.codeReplacer.camelToCss",
    "onCommand:extension.codeReplacer.stylesToClassName",
    "onCommand:extension.codeReplacer.classNameToStyles",
    "onCommand:extension.codeReplacer.cssToStyle",
    "onCommand:extension.codeReplacer.styleToCss"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.codeReplacer.cssToCamel",
        "title": "Transform Css To Camel Style"
      },
      {
        "command": "extension.codeReplacer.camelToCss",
        "title": "Transform Camel To Css Style"
      },
      {
        "command": "extension.codeReplacer.stylesToClassName",
        "title": "Transform Styles To ClassName"
      },
      {
        "command": "extension.codeReplacer.classNameToStyles",
        "title": "Transform ClassName To Styles"
      },
      {
        "command": "extension.codeReplacer.cssToStyle",
        "title": "Transform Css To Styles"
      },
      {
        "command": "extension.codeReplacer.styleToCss",
        "title": "Transform Styles To Css"
      }
    ]
  },
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "publish": "npm config set registry http://registry.npmjs.org/ && vsce publish && npm config set registry https://registry.npm.taobao.org"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.32.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "typescript": "^3.9.4"
  }
}
