{
  "name": "codereplacer",
  "displayName": "codeReplacer",
  "description": "replace css styles as you like",
  "version": "0.0.1",
  "publisher": "rebornix",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.codeReplacer.classNameToStyles",
    "onCommand:extension.codeReplacer.cssToCamel",
    "onCommand:extension.codeReplacer.camelToCss",
    "onCommand:extension.codeReplacer.stylesToClassName"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.codeReplacer.classNameToStyles",
        "title": "Transform ClassName To Styles"
      },
      {
        "command": "extension.codeReplacer.cssToCamel",
        "title": "Transform Css To Camel Style"
      },
      {
        "command": "extension.codeReplacer.camelToCss",
        "title": "Transform Camel To Css Style"
      },
      {
        "command": "extension.codeReplacer.stylesToClassName",
        "title": "Transform Styles To ClassName"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "typescript": "^3.9.4",
    "@types/vscode": "^1.32.0"
  }
}
